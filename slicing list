def reverse(a):
    return a[::-1]
code = ['d', 'r', '^', 'o', ']', 'Z', 'g', 'X', '|', 's', '@', 'x', ' ', 'T', 'U', 'a', 'v', 'x', 'r', 'j', 'B', 'e', 'l', 'O', ' ',
        'A', 'q', 'g', '-', 'B', 'o', 'y', 'D', 'o', 'A', 'S', 'd', 't', 'e']
extracted = code[::3]
print(extracted)
playlist = ['a', 'b', 'c', 'd', 'e']
print(reverse(playlist))
sandwich = ["Bread", "Lettuce", "Tomato", "Cheese", "Ham", "Bread"] 
breadless = sandwich[1:-1]
print(breadless)
